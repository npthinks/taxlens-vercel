{
    "version": 2,
    "builds": [
        {
            "src": "ask/api.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/ask",
            "dest": "ask/api.py"
        },
        {
            "src": "/",
            "dest": "ask/api.py"
        }
    ]
}